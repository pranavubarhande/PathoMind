<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:context=".InsidetestFragment1">

        <!-- TODO: Update blank fragment layout -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            >
            <TextView
                android:id="@+id/infrag1textview1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="Test Name: "/>
            <TextView
                android:id="@+id/infrag1textview2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mumbai University paper 1"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            >
            <TextView
                android:id="@+id/infrag1textview3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="Test Description: "/>
            <TextView
                android:id="@+id/infrag1textview4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mumbai University paper 1"/>
        </LinearLayout>
        <com.ortiz.touchview.TouchImageView
            android:id="@+id/infrag1image1"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:layout_marginTop="10dp"

            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.ortiz.touchview.TouchImageView
            android:id="@+id/infrag1image2"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.ortiz.touchview.TouchImageView
            android:id="@+id/infrag1image3"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.ortiz.touchview.TouchImageView
            android:id="@+id/infrag1image4"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.ortiz.touchview.TouchImageView
            android:id="@+id/infrag1image5"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <Button
            android:id="@+id/infrag1btn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="Upload"/>
        <Button
            android:id="@+id/infrag1btn2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="Confirm and Submit"/>
    </LinearLayout>
</ScrollView>



